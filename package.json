{
  "name": "flowstationkingdom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsc-node": "tsc -p tsconfig.json",
    "lint:js": "eslint src --ext .ts --ext .tsx --fix",
    "check-updates": "ncu -u",
    "check-updates-minor": "ncu -u -t minor",
    "generate:changelog": "node ./scripts/changelog.js",
    "preinstall": "rm -rf .husky",
    "postinstall": "cd .. && husky install web/.husky && husky add web/.husky/pre-commit \"cd web/ && npm run tsc-node && node_modules/.bin/lint-staged\"",
    "cz": "git cz",
    "clean": "rimraf ./cache && rimraf ./artifacts",
    "compile": "yarn clean && hardhat compile",
    "coverage": "yarn compile && hardhat coverage --temp artifacts",
    "deploy": "hardhat run scripts/deploy.js",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "test": "hardhat test test/*.ts",
    "test-clean": "yarn clean && yarn test",
    "test-fast": "TS_NODE_TRANSPILE_ONLY=1 hardhat test test/*.ts",
    "typechain": "hardhat typechain"
  },
  "lint-staged": {
    "{**/*,*}.{json,css,md}": [
      "prettier --write"
    ],
    "{**/*,*}.{ts,tsx,js,jsx}": [
      "eslint .eslintrc.js --fix --max-warnings=-1",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./fieryhartvim/scripts/commitizen"
    }
  },
  "extends": [
    "next",
    "prettier"
  ],
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "https": false,
    "http": false,
    "crypto": false,
    "stream": false,
    "net": false,
    "tls": false,
    "constants": false,
    "zlib": false,
    "child_process": false,
    "console": false,
    "async_hooks": false,
    "ethereum-waffle": false
  },
